<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        android:id="@+id/name_lbl_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:layout_marginTop="8dp"
        android:clickable="true"
        app:cardBackgroundColor="@color/gray_light"
        app:cardCornerRadius="16dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="32dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <TextView
                android:id="@+id/name_lbl_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/name_lbl_title_string"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/user_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/name_lbl_title" />

            <View
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentEnd="true"
                android:background="@drawable/ic_baseline_arrow_forward_ios_24" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/email_lbl_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:clickable="true"
        app:layout_constraintTop_toBottomOf="@id/name_lbl_container"
        app:cardBackgroundColor="@color/gray_light"
        app:cardCornerRadius="16dp"
        tools:layout_editor_absoluteX="32dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <TextView
                android:id="@+id/email_lbl_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/email_lbl_title_string"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/user_email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/email_lbl_title" />

            <View
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentEnd="true"
                android:background="@drawable/ic_baseline_arrow_forward_ios_24" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/password_lbl_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:clickable="true"
        app:layout_constraintTop_toBottomOf="@id/email_lbl_container"
        app:cardBackgroundColor="@color/gray_light"
        app:cardCornerRadius="16dp"
        tools:layout_editor_absoluteX="32dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <TextView
                android:id="@+id/password_lbl_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/password_lbl_title_string"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/user_password"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/password_lbl_title" />

            <View
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentEnd="true"
                android:background="@drawable/ic_baseline_arrow_forward_ios_24" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/address_lbl_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:clickable="true"
        app:layout_constraintTop_toBottomOf="@id/password_lbl_container"
        app:cardBackgroundColor="@color/gray_light"
        app:cardCornerRadius="16dp"
        tools:layout_editor_absoluteX="32dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <TextView
                android:id="@+id/address_lbl_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/address_lbl_title_string"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/user_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/address_lbl_title" />

            <View
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentEnd="true"
                android:background="@drawable/ic_baseline_arrow_forward_ios_24" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/phone_number_lbl_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:clickable="true"
        app:cardBackgroundColor="@color/gray_light"
        app:cardCornerRadius="16dp"
        app:layout_constraintTop_toBottomOf="@id/address_lbl_container"
        tools:layout_editor_absoluteX="32dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <TextView
                android:id="@+id/phone_number_lbl_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/phone_number_lbl_title_string"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/user_phone_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/phone_number_lbl_title" />

            <View
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentEnd="true"
                android:background="@drawable/ic_baseline_arrow_forward_ios_24" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/payment_lbl_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:clickable="true"
        app:layout_constraintTop_toBottomOf="@id/phone_number_lbl_container"
        app:cardBackgroundColor="@color/gray_light"
        app:cardCornerRadius="16dp"
        tools:layout_editor_absoluteX="32dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <TextView
                android:id="@+id/payment_lbl_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/payment_lbl_title_string"
                android:textStyle="bold" />

            <View
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentEnd="true"
                android:background="@drawable/ic_baseline_arrow_forward_ios_24" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/push_notifications_lbl_container"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_margin="32dp"
        android:clickable="true"
        app:layout_constraintTop_toBottomOf="@id/payment_lbl_container"
        app:cardBackgroundColor="@color/gray_light"
        app:cardCornerRadius="16dp"
        tools:layout_editor_absoluteX="32dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <TextView
                android:id="@+id/push_notifications_lbl_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/push_notifications_lbl_title_string"
                android:textStyle="bold" />

            <androidx.appcompat.widget.SwitchCompat
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/help_lbl_container"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_margin="32dp"
        android:clickable="true"
        app:layout_constraintTop_toBottomOf="@id/push_notifications_lbl_container"
        app:cardBackgroundColor="@color/gray_light"
        app:cardCornerRadius="16dp"
        tools:layout_editor_absoluteX="32dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <TextView
                android:id="@+id/help_lbl_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/help_lbl_title_string"
                android:textStyle="bold" />

            <View
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentEnd="true"
                android:background="@drawable/ic_baseline_arrow_forward_ios_24" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/logout_lbl_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:layout_marginBottom="8dp"
        android:clickable="true"
        app:cardBackgroundColor="@color/gray_light"
        app:cardCornerRadius="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/help_lbl_container"
        tools:layout_editor_absoluteX="32dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp">

            <TextView
                android:id="@+id/logout_lbl_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/logout_lbl_title_string"
                android:textStyle="bold" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>